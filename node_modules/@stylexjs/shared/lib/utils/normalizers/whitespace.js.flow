/**
 * Use single spaces and remove spaces when not needed: around functions,
 * commas. But preserve space around + and - as they are required in calc()
 */

declare export default function normalizeWhitespace(
  ast: PostCSSValueAST,
  _: mixed,
): PostCSSValueAST;
